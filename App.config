<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
<connectionStrings>
  <add name="AzureStorageAccount" connectionString="DefaultEndpointsProtocol=https;AccountName=<Storage Account Name>;AccountKey=<Paste access key 1 or key 2 here>"/>  
       </connectionStrings>
<appSettings>
  <add key="SourceFolder" value="c:\<local folder name>"/>  
   <add key="DestFolder" value="<public container name>"/>
</appSettings>
</configuration>
